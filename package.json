{
  "name": "softylines-webschool-v1",
  "version": "1.0.2",
  "description": "The architecture for nodejs backend application. It is build on top of expressjs using typescript.",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run serve",
    "start:sc": "npm run build && npm run serve:sc",
    "start:backup": "npm run build && npm run serve:backup",
    "dev": "ts-node-dev --ignore-watch src/apps/main/Routes.routes.ts src/apps/main/server.ts",
    "dev:sc": "ts-node-dev src/apps/smart-calendar/server.ts",
    "dev:backup": "ts-node-dev src/apps/backup/server.ts",
    "serve": "node --trace-warnings dist/apps/main/server.js",
    "serve:sc": "node dist/apps/smart-calendar/server.js",
    "serve:backup": "node dist/apps/backup/server.js",
    "build": "./dotenvcrab --strict --json && npm run clean && npm run buildRoutes && npm run build-ts",
    "clean": "rm -rf ./dist",
    "build-ts": "tsc -p tsconfig.json",
    "eslint": "eslint",
    "upgrade": "npm update --save-dev && npm update --save",
    "coverage": "jest --coverage --verbose",
    "test": "node --max-old-space-size=4096 ./node_modules/.bin/jest --colors --runInBand -b",
    "seed": "ts-node-dev  src/helpers/seeder.ts",
    "DB": "./importDB.sh",
    "app": "./run.sh",
    "restart": "docker restart backend nginx smart-calendar",
    "fix": "ts-node-dev src/scripts/index.ts",
    "executor": "ts-node ./src/core/scriptExecutor.ts",
    "buildRoutes": "ts-node src/apps/main/index.routes.ts",
    "build-types": "cd webschool-types && npm run build && cd ../"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Softylines",
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^1.3.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dropbox": "^10.34.0",
    "express": "^4.17.1",
    "firebase-admin": "^13.4.0",
    "iconv-lite": "^0.6.3",
    "inversify": "^6.0.2",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mongoose": "^7.0.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.1",
    "pino": "^8.16.0",
    "pino-pretty": "^10.2.3",
    "reflect-metadata": "^0.2.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "zod": "^3.22.4",
    "zod-validation-error": "^2.1.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.163",
    "@types/multer": "^1.4.7",
    "@types/node": "^22.7.4",
    "@types/node-schedule": "^2.1.0",
    "@types/nodemailer": "^6.4.7",
    "@types/uuid": "^9.0.8",
    "@types/validator": "^13.11.5",
    "@typescript-eslint/parser": "^8.9.0",
    "dotenv": "^8.2.0",
    "eslint": "^9.12.0",
    "eslint-plugin-max-params-no-constructor": "^0.0.4",
    "husky": "^9.0.11",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.9.0"
  }
}
