{
  "name": "@iecd/shared-types",
  "version": "1.0.0",
  "description": "Shared TypeScript types for IECD API routes and entities",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./enums": {
      "types": "./dist/enums/index.d.ts",
      "import": "./dist/enums/index.mjs",
      "require": "./dist/enums/index.js"
    },
    "./entities": {
      "types": "./dist/entities/index.d.ts",
      "import": "./dist/entities/index.mjs",
      "require": "./dist/entities/index.js"
    },
    "./routes": {
      "types": "./dist/routes/index.d.ts",
      "import": "./dist/routes/index.mjs",
      "require": "./dist/routes/index.js"
    },
    "./common": {
      "types": "./dist/common/index.d.ts",
      "import": "./dist/common/index.mjs",
      "require": "./dist/common/index.js"
    },
    "./route-metadata": {
      "types": "./dist/route-metadata/index.d.ts",
      "import": "./dist/route-metadata/index.mjs",
      "require": "./dist/route-metadata/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "typescript",
    "types",
    "api",
    "iecd"
  ],
  "author": "IECD Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  }
}
