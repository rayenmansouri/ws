{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist"
  },
  "include": [
    "src/**/*.ts",
    "seeders/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "**/*.test.ts",
    "**/*.spec.ts",
    ".templates",
    "src/__tests__/**/*",
    "iecd-types/**",
    "**/iecd-types/**"
  ]
}